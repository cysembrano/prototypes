@model FlowFB.Web.Models.SetupModel

@{
    ViewBag.Title = "Setup";
}

<h2>Setup</h2>
<div class="span6">
    <div class="widget">
        <div class="widget widget-table action-table">
            <div class="widget-header">
                <i class="icon-th-list"></i>
                <h3>Purchases Data</h3>
            </div>
            <!-- /widget-header -->
            <input type="hidden" id="thisURL" value='@Url.Action("SetupProjectUpdate", "Home")'/>
            <div class="widget-content">
                <table class="table table-striped table-bordered" id="tblSetup">
                    <thead>
                        <tr>
                            <th> Project</th>
                            <th> ID</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var kv in Model.Projects)
                        {
                            <tr>
                                <td class="kvkey">@kv.Key</td>
                                <td class="kvval" id="@kv.Key">@kv.Value.ToString()</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div> <!-- /widget-content -->
        </div>
    </div> <!-- /widget-content -->
</div> <!-- /widget -->

@section scripts
{
    <script src="~/Templates/js/edit-table/edit-table.js" type="text/javascript"></script>
    <script src="~/Scripts/Custom/Setup/ProjectSetup.js" type="text/javascript"></script>
}


